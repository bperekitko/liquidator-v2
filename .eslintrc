{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "parserVersion": 2020,
        "sourceType": "module"
    },
    "extends": [
        "plugin:@typescript-eslint/recommended"
    ],
    "ignorePatterns": "dist/*",
    "rules": {
        "@typescript-eslint/no-unused-vars": [
            "error",
            {
                "varsIgnorePattern": "_",
                "argsIgnorePattern": "_"
            }
        ]
    }
}